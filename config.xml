<?xml version='1.0' encoding='utf-8'?>
<widget id="com.gestion.coupe" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>gestion</name>
    <description>Sample Apache Cordova App</description>
    <author email="dev@cordova.apache.org" href="https://cordova.apache.org">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <platform name="android">
  <icon background="resources/android/icon/drawable-ldpi-icon.png" density="ldpi" foreground="resources/android/icon/mipmap-ldpi/icon.png" />
  <icon background="resources/android/icon/drawable-mdpi-icon.png" density="mdpi" foreground="resources/android/icon/mipmap-mdpi/icon.png" />
  <icon background="resources/android/icon/drawable-hdpi-icon.png" density="hdpi" foreground="resources/android/icon/mipmap-hdpi/icon.png" />
  <icon background="resources/android/icon/drawable-xhdpi-icon.png" density="xhdpi" foreground="resources/android/icon/mipmap-xhdpi/icon.png" />
  <icon background="resources/android/icon/drawable-xxhdpi-icon.png" density="xxhdpi" foreground="resources/android/icon/mipmap-xxhdpi/icon.png" />
  <icon background="resources/android/icon/drawable-xxxhdpi-icon.png" density="xxxhdpi" foreground="resources/android/icon/mipmap-xxxhdpi/icon.png" />
</platform>
</widget>
